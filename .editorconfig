# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

# Set default charset
[src/**.js]
indent_style = space
indent_size = 2

[*.json]
indent_style = space
indent_size = 2

[resource/pms/config.json]
indent_style = tab
indent_size = 4
